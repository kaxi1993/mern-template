{
  "extends": [
    "eslint:recommended",
    "prettier",
    "plugin:jest/recommended"
  ],
  "plugins": [
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "globals": {
    "process": true,
    "__dirname": true
  },
  "rules": {
    "semi": "error"
  }
}
